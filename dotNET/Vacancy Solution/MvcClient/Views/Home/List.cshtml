@model IEnumerable<MvcClient.VacancyServiceReference.Vacancy>

@{
    ViewBag.Title = "Результат поиска";
}

<div>
    <br />
    <br/>
    @foreach (var v in Model)
    {
        <div class="item">
            <h3>@v.Name</h3>
            <p><a href="@v.Url">@v.Url</a></p>
            <p>@v.Requirement</p>
            <p>@v.Responsibility</p>
            <br/>
        </div>
    }
</div>

